/* components/decorate/decorate.wxss */
.wrap {
  margin-top: 70rpx;
  box-sizing: border-box;
  padding-top: 30rpx;
  width: 100%;
  height: 80rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.block {
  content: '';
  width: 30%;
  height: 1rpx;
  border-bottom: 1px dashed #333;
  display: inline-block;
}
.wrap image {
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  margin: 0 20rpx;
}